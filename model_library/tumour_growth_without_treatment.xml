<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version2/core" level="3" version="2">
  <model id="tumour_growth_wothout_treatment" timeUnits="day">
    <listOfUnitDefinitions>
      <unitDefinition id="cm^3">
        <listOfUnits>
          <unit kind="meter" exponent="3" scale="-2" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="day">
        <listOfUnits>
          <unit kind="second" exponent="1" scale="0" multiplier="86400"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="1/day">
        <listOfUnits>
          <unit kind="second" exponent="-1" scale="0" multiplier="86400"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfParameters>
      <parameter id="tumour_volume" value="1" constant="false" units="cm^3"/>
      <parameter id="lambda" value="1" constant="true" units="1/day"/>
      <parameter id="critical_volume" value="1" constant="true" units="cm^3"/>
    </listOfParameters>
    <listOfRules>
      <rateRule variable="tumour_volume">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> lambda </ci>
              <ci> critical_volume </ci>
              <ci> tumour_volume </ci>
            <apply>
              <plus/>
              <ci> tumour_volume </ci>
              <ci> critical_volume </ci>
        </math>
      </rateRule>
    </listOfRules>
  </model>
</sbml>
